{
  "name": "Tortoise",
  "description": "A JavaScript-based implementation of the NetLogo simulation engine",
  "repository": "https://github.com/NetLogo/Tortoise",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build:prod": "node linter.js && webpack --config webpack.config.js --mode production",
    "build": "webpack --progress --config webpack.config.js",
    "clean": "rm -rf target/classes/js/*",
    "start": "webpack serve --open",
    "wasm:node": "cd src/main/rust && wasm-pack build --target nodejs",
    "wasm:webpack": "cd src/main/rust && wasm-pack build",
    "wasm:clean": "rm -rf src/main/rust/pkg/*",
    "test": "cross-env TS_NODE_PROJECT=\"tsconfig.test.json\" mocha -t 0 -r ts-node/register test/*.ts"
  },
  "dependencies": {
    "brazierjs": "2.2.0",
    "buffer": "^6.0.3",
    "csv-parse": "4.6.5",
    "iframe-phone": "1.2.0",
    "mori": "0.3.2",
    "stream-browserify": "^3.0.0",
    "tone": "14.7.39",
    "util": "^0.12.3",
    "vectorious": "6.0.0-beta.11"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/java": "^0.9.1",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.25",
    "chai": "^4.2.0",
    "coffee-loader": "^2.0.0",
    "coffeelint": "2.1.0",
    "coffeelint-stylish": "^0.1.2",
    "coffeescript": "^2.5.0",
    "cross-env": "^7.0.3",
    "expose-loader": "^1.0.3",
    "grunt": "^1.3.0",
    "grunt-coffeelint": "^0.0.16",
    "java": "^0.12.1",
    "mocha": "^8.2.1",
    "node-glob": "^1.2.0",
    "ts-loader": "^8.0.14",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "vm2": "^3.9.2",
    "webpack": "^5.15.0",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.2"
  }
}
